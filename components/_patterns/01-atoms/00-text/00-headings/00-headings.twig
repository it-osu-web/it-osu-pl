<div class="sg-header-styles">
	{% for i in 1..6 %}
		<div class="sg-pattern pl-heading-examples">
			<h4 class="sg-pattern-title">h{{ i }}</h4>
			<h{{i}} class="h{{ i }}">This is a Heading
				{{ i }}
				example</h{{i}}>
		</div>
	{% endfor %}
</div>
{% if meta %}
	<p>
		<small>{{ meta.description }}</small>
	</p>
{% endif %}
